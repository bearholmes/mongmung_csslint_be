    "posthtml": "^0.16.6",
    "posthtml-attrs-sorter": "^1.1.0",
    "serve-favicon": "^2.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^3.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
